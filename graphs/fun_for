digraph {
	i0 [label=start color=green shape=invhouse]
	i1 [label="int a = 1"]
	i2 [label="int b = 0"]
	i3 [label="for (int i = 0; i < 10; i++)" color=orange]
	i4 [label="a *= i"]
	i5 [label="if (a % 2 == 0)" color=red]
	i6 [label=then color=red]
	i5 -> i6 [color=blue constraint=true]
	i7 [label="b++"]
	i6 -> i7 [color=blue constraint=true]
	i8 [label=else color=red]
	i5 -> i8 [color=blue constraint=true]
	i9 [label="b--"]
	i8 -> i9 [color=blue constraint=true]
	i3 -> i4 [color=blue constraint=true]
	i3 -> i5 [color=blue constraint=true]
	i10 [label="a = 100"]
	i0 -> i1 [color=blue constraint=true]
	i0 -> i2 [color=blue constraint=true]
	i0 -> i3 [color=blue constraint=true]
	i0 -> i10 [color=blue constraint=true]
}
