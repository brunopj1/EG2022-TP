	i9 -> i10 [color=blue constraint=true]
	i11 [label=else color=red]
	i8 -> i11 [color=blue constraint=true]
	i12 [label="b++"]
	i13 [label="a--"]
	i11 -> i12 [color=blue constraint=true]
